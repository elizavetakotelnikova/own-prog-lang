array bool isPrime[10000] = {true, true, true, true, true, true, true, true, true, true, true, true, true, true, true, true, true, true, true, true, true, true, true, true, true, true, true, true, true, true, true, true};
isPrime[0] = false;
isPrime[1] = false;

for (int p = 2; p * p <= 1000; p = p + 1) {
