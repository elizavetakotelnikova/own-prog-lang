array int arr[4] = {-62, -243, -430, -204};

int n = 4;
int swap = 0;
for (int i = 0; i < n - 1; i = i + 1){
    for (int j = 0; j < n - i - 1; j = j + 1){
        if (arr[j] > arr[j + 1]){
            swap = arr[j];
            arr[j] = arr[j + 1];
            arr[j + 1] = swap;
        }
        print(n);
    }
}


for (int k = 0; k < n ; k = k + 1){
    print(arr[k]);
}
